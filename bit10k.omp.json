{
   "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
   "version": 2,
   "blocks": [
      {
         "alignment": "left",
         "type": "prompt",
         "segments": [
            {
               "foreground": "#a89883",
               "style": "plain",
               "template": "\u256d\u2500",
               "type": "session"
            },
            {
               "background": "#19191a",
               "powerline_symbol": "",
               "style": "powerline",
               "template": " <#c95110>{</> <#00c7ff>{{ .UserName }}</><#c95110> @</><#72cba2>{{ .HostName }} </><#c95110>}</> ",
               "type": "session"
            }
         ]
      },
      {
         "alignment": "right",
         "filler": "\u00b7",
         "overflow": "hide",
         "type": "prompt",
         "segments": [
            {
               "background": "#d80c56",
               "foreground": "#19191a",
               "powerline_symbol": "",
               "style": "powerline",
               "template": " {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ⎈ ",
               "type": "kubectl"
            },
            {
               "foreground": "#a89883",
               "style": "plain",
               "template": "\u2500\u256e",
               "type": "session"
            }
         ]
      },
      {
         "alignment": "left",
         "newline": true,
         "type": "prompt",
         "segments": [
            {
               "foreground": "#a89883",
               "style": "plain",
               "template": "\ufe31\u035e",
               "type": "session"
            },
            {
               "background": "#19191a",
               "powerline_symbol": "",
               "properties": {
                  "macos": "<#0be1e2>\uf179</> "
               },
               "style": "powerline",
               "type": "os"
            },
            {
               "background": "#549ce2",
               "foreground": "#19191a",
               "powerline_symbol": "",
               "properties": {
                  "home_icon": " ~",
                  "style": "folder"
               },
               "style": "powerline",
               "type": "path"
            },
            {
               "background": "#87c723",
               "foreground": "#19191a",
               "powerline_symbol": "",
               "properties": {
                  "branch_icon": "\ue725 ",
                  "fetch_status": true,
                  "fetch_upstream_icon": true,
                  "github_icon": "\uf408 "
               },
               "style": "powerline",
               "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
               "type": "git"
            }
         ]
      },
      {
         "alignment": "right",
         "overflow": "hide",
         "type": "prompt",
         "segments": [
            {
               "background": "#51b8d1",
               "foreground": "#19191a",
               "powerline_symbol": "",
               "style": "powerline",
               "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}{{.Version}}  {{ end }}{{ end }} ",
               "type": "project"
            },
            {
               "background": "#19191a",
               "foreground": "#86c623",
               "powerline_symbol": "",
               "properties": {
                  "fetch_package_manager": true,
                  "npm_icon": " <#cc3a3a> </> ",
                  "yarn_icon": " <#348cba>\ue6a7</>"
               },
               "style": "powerline",
               "template": " {{ .PackageManagerIcon }}{{ .Full }}   ",
               "type": "node"
            },
            {
               "foreground": "#FFE873",
               "style": "plain",
               "template": "<#a89883>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<#a89883>)</>",
               "type": "python"
            },
            {
               "foreground": "#ec2729",
               "style": "plain",
               "template": "<#a89883>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#a89883>)</>",
               "type": "java"
            },
            {
               "foreground": "#0d6da8",
               "style": "plain",
               "template": "<#a89883>(</>{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}<#a89883>)</>",
               "type": "dotnet"
            },
            {
               "foreground": "#06aad5",
               "style": "plain",
               "template": "<#a89883>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#a89883>)</>",
               "type": "go"
            },
            {
               "foreground": "#925837",
               "style": "plain",
               "template": "<#a89883>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#a89883>)</>",
               "type": "rust"
            },
            {
               "foreground": "#055b9c",
               "style": "plain",
               "template": "<#a89883>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#a89883>)</>",
               "type": "dart"
            },
            {
               "foreground": "#ce092f",
               "style": "plain",
               "template": "<#a89883>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#a89883>)</>",
               "type": "angular"
            },
            {
               "foreground": "#a89883",
               "style": "plain",
               "template": "<#1e293b>(</>{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}<#1e293b>)</>",
               "type": "nx"
            },
            {
               "foreground": "#359a25",
               "style": "plain",
               "template": "<#a89883>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#a89883>)</>",
               "type": "julia"
            },
            {
               "background": "#d80c56",
               "foreground": "#19191a",
               "powerline_symbol": "",
               "style": "powerline",
               "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}   ",
               "type": "ruby"
            },
            {
               "foreground": "#5398c2",
               "style": "plain",
               "template": "<#a89883>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#a89883>)</>",
               "type": "azfunc"
            },
            {
               "foreground": "#faa029",
               "style": "plain",
               "template": "<#a89883>(</>{{.Profile}}{{if .Region}}@{{.Region}}{{end}}<#a89883>)</>",
               "type": "aws"
            },
            {
               "foreground": "#a89883",
               "foreground_templates": [
                  "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
                  "{{if eq \"Discharging\" .State.String}}#fffb38{{end}}",
                  "{{if eq \"Full\" .State.String}}#33dd2d{{end}}"
               ],
               "properties": {
                  "charged_icon": "\uf240 ",
                  "charging_icon": "\uf1e6 ",
                  "discharging_icon": "\ue234 "
               },
               "style": "plain",
               "template": "<#a89883>[</>{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}%<#a89883>]\u2500</>",
               "type": "battery"
            },
            {
               "foreground": "#a89883",
               "style": "plain",
               "template": "\u035e\ufe31",
               "type": "session"
            }
         ]
      },
      {
         "alignment": "left",
         "newline": true,
         "type": "prompt",
         "segments": [
            {
               "style": "plain",
               "template": "\u2570\u2500",
               "type": "text"
            },
            {
               "foreground": "#53c007",
               "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
               "properties": {
                  "always_enabled": true
               },
               "style": "plain",
               "template": "\ue285$\ueb9e ",
               "type": "status"
            }
         ]
      },
      {
         "alignment": "right",
         "type": "prompt",
         "segments": [
            {
               "background": "#9bbe55",
               "foreground": "#19191a",
               "powerline_symbol": "",
               "properties": {
                  "time_format": "15:04"
               },
               "style": "powerline",
               "template": " {{ .CurrentDate | date .Format }}  ",
               "type": "time"
            },
            {
               "style": "plain",
               "template": "\u2500\u256f",
               "type": "text"
            }
         ]
      }
   ]
}
